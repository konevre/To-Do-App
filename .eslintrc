{
    "extends": [
        "react-app",
        "react-app/jest",
        "eslint:recommended", "plugin:import/errors", "plugin:import/warnings"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "indent": ["warn", 4],
        "import/order": [
            "error",
            {
                "groups": [
                    "builtin",
                    "external",
                    "internal",
                    "parent",
                    "sibling",
                    "index",
                    "object",
                    "type"
                ],
                "newlines-between": "always-and-inside-groups"
            }
        ]
    },
    "plugins": ["import"]
}